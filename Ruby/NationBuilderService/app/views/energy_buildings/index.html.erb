<h1>Listing energy_buildings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Poweroutput</th>
      <th>Type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @energy_buildings.each do |energy_building| %>
      <tr>
        <td><%= energy_building.name %></td>
        <td><%= energy_building.poweroutput %></td>
        <td><%= energy_building.type %></td>
        <td><%= link_to 'Show', energy_building %></td>
        <td><%= link_to 'Edit', edit_energy_building_path(energy_building) %></td>
        <td><%= link_to 'Destroy', energy_building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Energy building', new_energy_building_path %>
